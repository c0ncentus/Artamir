{
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "target": "ES2020",
    "module": "commonjs",
    "lib": [
      // "ES2023",
      "ES2022",
      "ES2021",
      "ES2020",
      "ES2019",
      "ES2018",
      "ES2017",
      "ES2016",
      "ES2015",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "checkJs": false,
    "incremental": true,
    "declaration": true,
    "pretty": true,
    "diagnostics": true,
    "removeComments": false,

  },
  "include": [
    "src"
  ]
}